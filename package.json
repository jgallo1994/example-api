{
  "name": "example-api",
  "version": "1.0.0",
  "description": "REST API built with Node.js, TypeScript, and DDD principles",
  "main": "dist/presentation/api-main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/presentation/api-main.js",
    "start-dev": "ts-node-dev --respawn src/presentation/api-main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "prestart": "npm run build"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "ddd",
    "clean-code",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "inversify": "^7.9.1",
    "joi": "^18.0.1",
    "mongodb": "^6.3.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.27.8",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
